{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "abl",
      "camelcase": "abl",
      "title": "abl",
      "scope": "source.abl",
      "file-types": ["cls", "p", "i"],
      "injection-regex": "^abl$",
      "class-name": "TreeSitterAbl",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.0.50",
    "license": "MIT",
    "description": "ABL grammar for tree-sitter",
    "authors": [
      {
        "name": "Kamil Jakubus"
      }
    ],
    "links": {
      "repository": "https://github.com/usagi-coffee/tree-sitter-abl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
