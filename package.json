{
  "name": "@usagi-coffee/tree-sitter-abl",
  "version": "0.0.49",
  "description": "ABL grammar for tree-sitter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usagi-coffee/tree-sitter-abl.git"
  },
  "license": "MIT",
  "author": {
    "name": "Kamil Jakubus"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": ["incremental", "parsing", "tree-sitter", "abl"],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "prebuildify": "^6.0.1",
    "tree-sitter": "^0.22.4",
    "tree-sitter-cli": "^0.26.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "bun run tools/generate.js && bun run tools/test.js",
    "generate": "tree-sitter generate && bun run check",
    "check": "bun run tools/generate.js && bun run tools/check.js",
    "check:statements": "bun run tools/check_statements.js",
    "parse": "bun run tools/generate.js && tree-sitter parse",
    "parse:snippet": "bun run tools/generate.js && bun run tools/parse.js",
    "reference": "bun run tools/reference.js",
    "build": "tree-sitter generate && tree-sitter build",
    "build:wasm": "tree-sitter generate && tree-sitter build -w",
    "dev": "nodemon --ignore ./src -e js,json,p --exec 'tree-sitter generate && tree-sitter parse example.p'",
    "prepublishOnly": "bun run prepublish.js"
  }
}
